
filetype plugin on
filetype indent on

colorscheme elflord

set number

set autoread

set wildmenu

set ruler

set backspace=eol,start,indent

set ignorecase

set smartcase

set hlsearch

set incsearch

set lazyredraw

set magic

set foldcolumn=1

syntax enable

set ffs=unix,dos,mac

set nobackup
set nowb
set noswapfile

set expandtab
set smarttab
set tabstop=4
set shiftwidth=4

set ai
set si
set wrap


